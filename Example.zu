-- | Defines natural numbers without induction feature, it's
-- like functions in dependent langauges.
nat : _ = (N : \Type) -> (N -> N) -> N -> N.

-- | Defines the zero constructor
Zero : _ = \fun a, a, zero ->
  zero.

-- | Defines the succ constructor
Succ : _ = \fun n, N, succ, _ ->
  (n N succ zero).
